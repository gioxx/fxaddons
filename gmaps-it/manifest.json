{

  "manifest_version": 2,
  "name": "GoogleMaps-IT",
  "description": "Cerca quello che ti serve su Google Maps (IT)",
  "version": "1.0",
  "applications": {
    "gecko": {
      "strict_min_version": "55.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "GoogleMaps-IT",
      "search_url": "https://www.google.it/maps/search/{searchTerms}",
      "keyword": "gmaps",
      "favicon_url": "https://hub.gfsolone.com/favicon/gmaps-16.ico",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }

}