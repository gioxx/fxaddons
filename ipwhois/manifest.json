{

  "manifest_version": 2,
  "name": "IPwhois",
  "description": "Cerca le informazioni relative a un indirizzo IP",
  "version": "1.0",
  "applications": {
    "gecko": {
      "strict_min_version": "55.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "IPwhois",
      "search_url": "https://myip.ms/info/whois/{searchTerms}",
      "keyword": "ipwhois",
      "favicon_url": "https://hub.gfsolone.com/favicon/ipwhois-16.ico",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }

}
