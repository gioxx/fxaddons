{

  "manifest_version": 2,
  "name": "StopForumSpam Search",
  "description": "Verifica se un utente del tuo forum è già stato segnalato per Spam",
  "version": "1.0",
  "applications": {
    "gecko": {
      "strict_min_version": "55.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "StopForumSpam",
      "search_url": "https://www.stopforumspam.com/search?q={searchTerms}",
      "keyword": "forumspam",
      "favicon_url": "https://www.stopforumspam.com/favicon.ico",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }

}